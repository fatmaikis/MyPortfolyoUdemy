@model List<Contact>
<section id="contact" class="s-contact target-section">

    <div class="overlay"></div>

    <div class="row narrow section-intro">
        <div class="col-full">
            <h3>İletişim</h3>
            <h1>Bana Ulaşın</h1>

            <p class="lead">Herhangi bir sorunuz ya da proje ile ilgili bir görüşme talebiniz için benimle iletişime geçmekten çekinmeyin. Aşağıdaki iletişim kanallarım üzerinden bana ulaşabilirsiniz. Size yardımcı olmak için buradayım!</p>
        </div>
    </div>

    <div class="row contact__main">
        @await Component.InvokeAsync("_MessageComponentPartial")

        <div class="col-four tab-full contact__infos">


            @foreach (var item in Model)
            {
                <h4 class="h06">@item.Title</h4>
                <p>
                    @item.Email1
                    <br />
                    @item.Phone1
                    <br />
                    @item.Address
                </p>
               
               
            }
           

        </div>

    </div>

</section> 